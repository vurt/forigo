newField_nodeClick:function(event,treeId,treeNode,clickFlag){
	$scope.$apply(function(){
		$params.selectedPath=treeNode.id;
		$params.isSelectedFolder=treeNode.isParent;
		$params.selectNode=treeNode;
	});
},
lastHeartBeat_customFormatter:function(cellvalue,options,rowObject){
	var lastHeartBeat=moment(cellvalue);
	var tooltip=&quot;最后心跳时间：&quot;+lastHeartBeat.format('YYYY-MM-DD, hh:mm:ss');
	if(moment().isBefore(lastHeartBeat.add(1,'m'))){
		//最后一次心跳时间与当前时间的间隔小于一分钟
		return &quot;&lt;span class='label label-success arrowed' title='&quot;+tooltip+&quot;'>在线&lt;/span>&quot;;
	}else{
		return &quot;&lt;span class='label label-danger arrowed-in' title='&quot;+tooltip+&quot;'>离线&lt;/span>&quot;;
	}
},

page_beforeDataSourceLoad:function($params){
	$params.isSelectedFolder=false;
	$params.selectedPath=&quot;未选中任何文件&quot;;
},


